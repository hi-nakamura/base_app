<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
  <div>
    <%= f.label :user_id %>
    <%= f.text_field :user_id, :autofocus => true %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div><%= f.submit "ログイン", :class => "btn" %></div>
<% end %>
